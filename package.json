{
  "name": "scilent-x",
  "private": true,
  "scripts": {
    "changeset": "pnpm exec changeset",
    "build": "turbo build",
    "dev": "turbo dev",
    "dev:ui": "pnpm sandbox --filter @scilent-one/ui",
    "storybook:ui": "pnpm turbo storybook --filter=@scilent-one/ui",
    "storybook:h-ui": "pnpm turbo storybook --filter=@scilent-one/scilent-ui",
    "lint": "turbo lint",
    "format": "turbo format",
    "typecheck": "turbo typecheck",
    "test": "turbo test",
    "test:coverage": "turbo test:coverage",
    "fix": "turbo lint && turbo format && turbo typecheck",
    "version": "pnpm exec changeset version",
    "release": "pnpm exec changeset publish",
    "clean:full": "rm -rf node_modules apps/*/node_modules apps/web/.next packages/*/node_modules .turbo apps/*/.turbo packages/*/.turbo",
    "reinstall": "pnpm clean:full && pnpm install"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.8",
    "@scilent-one/tooling": "workspace:*",
    "@turbo/gen": "^2.7.4",
    "eslint": "^9.39.2",
    "eslint-plugin-storybook": "^10.1.11",
    "pino-pretty": "^13.1.3",
    "prettier": "^3.8.0",
    "turbo": "^2.7.4",
    "typescript": "catalog:typescript"
  },
  "packageManager": "pnpm@10.28.0",
  "workspaces": [
    "apps/*",
    "packages/*"
  ]
}
